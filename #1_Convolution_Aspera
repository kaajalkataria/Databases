-- create table for Aspera Bike Design
-- CREATE DATABASE Aspera;
-- USE Aspera;
CREATE TABLE Aspera_Custom (
Model_No INTEGER NOT NULL PRIMARY KEY,
Color TEXT NOT NULL,
Size TEXT NOT NULL,
Graphics TEXT NOY NULL,
Frameset VARCHAR(1000) NOT NULL,
Price INTEGER NOT NULL
);

