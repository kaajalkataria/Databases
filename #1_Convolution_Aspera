-- Displaying Aspera Table with customer requirements
-- Requirement 1 : XS Cycle under Rs 70000
SELECT * FROM Aspera_Custom WHERE Size = 'XS' AND Price < 70000;

-- Requirement 2 : Minimum Price Cycles
SELECT MIN(Price) FROM Aspera_Custom;
SELECT * FROM Aspera_Custom WHERE Price = 35900;

-- Requirement 3 : Maximum Price Cycles
SELECT MAX(Price) FROM Aspera_Custom;
SELECT * FROM Aspera_Custom WHERE Price = 95750;

-- Create table for customer data
CREATE TABLE Customer_Data (
Model_No INTEGER NOT NULL,
Name TEXT NOT NULL,
Bill_No INTEGER NOT NULL,
FOREIGN KEY (Model_No) REFERENCES Aspera_Custom (Model_No)
);

-- Inserting Values in Customer_Data
INSERT INTO Customer_Data VALUES (229,'Kaajal Kataria',001);
INSERT INTO Customer_Data VALUES (141,'Ajinkya Nimbalkar',002);
INSERT INTO Customer_Data VALUES (1,'Rushika Kshirsagar',003);
INSERT INTO Customer_Data VALUES (109,'Kshitija Godase',004);
INSERT INTO Customer_Data VALUES (151,'Pia Katkar',005);

-- Displaying the cycles bought by 5 customers
SELECT * FROM Customer_Data,Aspera_Custom WHERE Customer_Data.Model_No = Aspera_Custom.Model_No;

-- Displaying the least expensive cycle sold
SELECT MIN(Price),Bill_No,Name FROM Aspera_Custom,Customer_Data WHERE Aspera_Custom.Model_No = Customer_Data.Model_No;

-- Displaying most expensive cycle sold
SELECT MAX(Price),Bill_No,Name FROM Aspera_Custom,Customer_Data WHERE Aspera_Custom.Model_No = Customer_Data.Model_No;
